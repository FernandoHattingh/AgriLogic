{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Authentication": {
    "CookieScheme": {
      "Name": "AgriLogic.Authentication",
      "LoginPath": "/Home/Index",
      "AccessDeniedPath": "/Home/AccessDenied"
    }
  },
  "Session": {
    "CookieName": "AgriLogic.Session",
    "IdleTimeoutMinutes": 30
  }
}
